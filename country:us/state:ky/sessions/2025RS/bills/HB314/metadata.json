  "identifier": "HB314",
  "title": "AN ACT relating to equality in access to financial services.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Attorney General",
    "Banks and Financial Institutions",
