{
  "legislative_session": "2025RS",
  "identifier": "HB114",
  "title": "AN ACT relating to landowner liability for recreational use permission.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Amusements and Recreation",
    "Athletics",
    "Civil Actions",
