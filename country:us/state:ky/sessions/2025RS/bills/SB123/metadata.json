  "identifier": "SB123",
  "title": "AN ACT relating to insurance coverage of roofing materials.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Insurance",
    "Local Mandate",
    "Property",
    "Real Estate"
  ],
  "abstracts": [
    {
      "note": "Summary of Original Version",
      "abstract": "Create a new section of Subtitle 12 of KRS Chapter 304 to require insurers to replace the roofing materials on the entire roof when uniformity of appearance cannot be achieved due to the age, condition, or discontinued manufacture of the existing roofing materials; prohibit the application of line of sight rules to exclude coverage for the replacement of roofing materials for any portion of the roof; provide that the Act applies to insurance policies, certificates, and contracts issued or renewed on or after the effective date of the Act."
    }
  ],
