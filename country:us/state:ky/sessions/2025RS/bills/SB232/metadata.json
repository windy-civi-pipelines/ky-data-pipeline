  "identifier": "SB232",
  "title": "AN ACT relating to insurance disclosures to dog bite and dog attack victims.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Civil Actions",
