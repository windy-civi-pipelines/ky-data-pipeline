  "legislative_session": "2025RS",
  "identifier": "HB298",
  "title": "AN ACT relating to schools identified for comprehensive support and improvement in schools.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Education, Elementary and Secondary",
    "State Agencies",
    "Teachers",
    "Textbooks"
  ],
  "abstracts": [
    {
      "note": "Summary of Original Version",
      "abstract": "Amend KRS 160.346 to require the Kentucky Department of Education to annually identify schools for comprehensive support and improvement (CSI); require the audit of a CSI school to include an assessment and recommendation regarding the principal's capacity to lead the turnaround effort; remove the required use of a turnaround vendor by a CSI school; require the department to establish professional learning for teachers in a CSI school; require the superintendent of the district in which the CSI school is located to adopt evidence-based curriculum and select high-quality instructional materials for the school."
    }
  ],
