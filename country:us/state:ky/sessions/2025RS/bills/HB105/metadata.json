{
  "legislative_session": "2025RS",
  "identifier": "HB105",
  "title": "AN ACT proposing to create a new section of the Constitution of Kentucky relating to the possession of cannabis.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Cannabis",
    "Constitution, Ky.",
    "Corrections Impact",
