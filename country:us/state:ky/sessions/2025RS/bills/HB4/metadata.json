  "legislative_session": "2025RS",
  "identifier": "HB4",
  "title": "AN ACT relating to initiatives regarding diversity, equity, and inclusion.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Attorney General",
    "Civil Actions",
    "Civil Rights",
