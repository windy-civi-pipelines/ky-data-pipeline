{
  "legislative_session": "2025RS",
  "identifier": "SJR55",
  "title": "A JOINT RESOLUTION directing Kentucky's public postsecondary institutions to combat antisemitism.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "joint resolution"
  ],
  "subject": [
    "Civil Rights",
