{
  "legislative_session": "2025RS",
  "identifier": "HB398",
  "title": "AN ACT relating to occupational safety and health.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Administrative Regulations and Proceedings",
    "Federal Laws and Regulations",
    "Labor and Industry",
