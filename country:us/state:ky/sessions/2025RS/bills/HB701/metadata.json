  "identifier": "HB701",
  "title": "AN ACT relating to blockchain digital assets.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Energy",
    "Information Technology",
