  "identifier": "HB423",
  "title": "AN ACT relating to prior authorization.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Actuarial Analysis",
    "Health Benefit Plan Mandate",
