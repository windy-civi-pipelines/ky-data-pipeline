{
  "legislative_session": "2025RS",
  "identifier": "HB695",
  "title": "AN ACT relating to the Medicaid program and declaring an emergency.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Effective Dates, Emergency",
    "General Assembly",
    "Health and Medical Services",
