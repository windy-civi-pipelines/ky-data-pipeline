{
  "legislative_session": "2025RS",
  "identifier": "HB315",
  "title": "AN ACT relating to the acquisition of agricultural land.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Agriculture",
    "Attorney General",
