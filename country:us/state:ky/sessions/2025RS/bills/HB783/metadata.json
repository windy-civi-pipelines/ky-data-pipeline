  "identifier": "HB783",
  "title": "AN ACT relating to temporary motor vehicle tags.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Corrections Impact",
    "County Clerks",
