  "identifier": "SB182",
  "title": "AN ACT relating to liability for injury or damage caused by the underage consumption of intoxicating substances.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Alcoholic Beverages",
    "Children and Minors",
