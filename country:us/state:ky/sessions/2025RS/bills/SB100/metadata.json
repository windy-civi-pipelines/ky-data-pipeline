{
  "legislative_session": "2025RS",
  "identifier": "SB100",
  "title": "AN ACT relating to tobacco, nicotine, or vapor product licensure.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Boards and Commissions",
    "Drugs and Medicines",
    "Fiscal Note",
