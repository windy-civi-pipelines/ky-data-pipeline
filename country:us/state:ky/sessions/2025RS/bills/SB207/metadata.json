{
  "legislative_session": "2025RS",
  "identifier": "SB207",
  "title": "AN ACT relating to education.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Boards and Commissions",
