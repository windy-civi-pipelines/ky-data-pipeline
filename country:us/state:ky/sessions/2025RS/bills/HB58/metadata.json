  "abstracts": [
    {
      "note": "Summary of Original Version",
      "abstract": "Create a new section of KRS Chapter 383 to permit a tenant to terminate a lease or rental agreement if he or she is seeking relocation due to a recent mental or physical health emergency; require the tenant to pay any rent due prorated to the effective date of termination; relieve the tenant from liability for any other rent or fees resulting from the termination."
    }
  ],
