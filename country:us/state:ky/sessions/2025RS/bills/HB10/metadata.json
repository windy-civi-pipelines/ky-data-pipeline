  "legislative_session": "2025RS",
  "identifier": "HB10",
  "title": "AN ACT relating to the rights of real property owners.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Civil Actions",
    "Corrections Impact",
