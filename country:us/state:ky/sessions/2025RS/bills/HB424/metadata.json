{
  "legislative_session": "2025RS",
  "identifier": "HB424",
  "title": "AN ACT relating to employment at public postsecondary education institutions.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Boards and Commissions",
    "Education, Higher",
    "Universities and Colleges"
  ],
  "abstracts": [
    {
      "note": "Summary of Original Version",
      "abstract": "Amend KRS 164.360, 164.230, and 164.830 to require the boards of each state university and the Kentucky Community and Technical College System to approve a performance and productivity evaluation process for presidents and all faculty members by January 1, 2026; require faculty evaluations to be completed at least once every four years; permit removal of certain employees for failure to meet performance and productivity requirements regardless of status; permit a board to delegate appointment and removal of faculty to the college or university president; require that each board of regents of the six state comprehensive universities appoint a university president; specify employment contract duration limits for employees of comprehensive universities and the Kentucky Community and Technical College System."
    }
  ],
