  "legislative_session": "2025RS",
  "identifier": "SB17",
  "title": "AN ACT relating to freestanding birthing centers.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Administrative Regulations and Proceedings",
    "Health and Medical Services",
