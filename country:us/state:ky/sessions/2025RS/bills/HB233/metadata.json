  "legislative_session": "2025RS",
  "identifier": "HB233",
  "title": "AN ACT relating to the prevention of harmful practices associated with property and casualty insurance.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
