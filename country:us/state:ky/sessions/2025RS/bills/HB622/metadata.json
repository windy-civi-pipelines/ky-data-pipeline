  "identifier": "HB622",
  "title": "AN ACT relating to fiscal matters and declaring an emergency.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Administrative Regulations and Proceedings",
    "Contracts",
