{
  "legislative_session": "2025RS",
  "identifier": "SB63",
  "title": "AN ACT relating to street-legal special purpose vehicles.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Administrative Regulations and Proceedings",
    "County Clerks",
